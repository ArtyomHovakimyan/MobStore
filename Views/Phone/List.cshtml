@model IEnumerable<MobStore.Models.Phone>
@{ 
	ViewData["Title"] = "Show all";
}

@{ 
	foreach(var phone in Model)
	{
		<div class="col-lg-3">
			<p>
				<img class="img-responsive" src="~/images/@phone.ImageUrl"/>
			</p>
			<h3>@phone.Price.ToString("c")</h3>
			<div>
				<p>@phone.Title</p>
			</div>
		</div>
	}
}